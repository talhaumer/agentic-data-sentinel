{
  "version": 2,
  "builds": [
    {
      "src": "vercel_main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel_main.py"
    },
    {
      "src": "/health",
      "dest": "/vercel_main.py"
    },
    {
      "src": "/docs",
      "dest": "/vercel_main.py"
    },
    {
      "src": "/",
      "dest": "/vercel_main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "vercel_main.py": {
      "maxDuration": 30
    }
  }
}
